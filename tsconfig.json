{
	"compilerOptions": {
		"module": "commonjs",
		"target": "esnext",
		"lib": ["esnext"],
		"sourceMap": true,
		"strict": true,
		"types": ["typescript-to-lua/language-extensions"]
	},
	"exclude": [
		"node_modules"
	],
	"tstl": {
		"luaTarget": "5.1",
		"noHeader": true,
		"luaLibImport": "require"
	}
}
